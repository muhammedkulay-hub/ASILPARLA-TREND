        "url": "https://api.asilparla.local/api/orders/sync",
        "method": "POST",
        "responseFormat": "json",
        "jsonParameters": true,
        "options": {
          "timeout": 30
        },
        "bodyParametersJson": "{\"source\":\"trendyol\",\"runBy\":\"n8n\"}"
      }
    },
    {
      "id": "log-node",
      "name": "Execution Log",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [780, 300],
